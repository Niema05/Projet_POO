# Configuration pour la base de données MySQL
# Modifiez ces paramètres selon votre environnement

# Hôte de la base de données
DATABASE_HOST=localhost

# Port MySQL (par défaut : 3306)
DATABASE_PORT=3306

# Nom de la base de données
DATABASE_NAME=bibliotheque

# Utilisateur MySQL
DATABASE_USER=root

# Mot de passe MySQL
DATABASE_PASSWORD=

# URL complète (générée automatiquement)
# jdbc:mysql://localhost:3306/bibliotheque
